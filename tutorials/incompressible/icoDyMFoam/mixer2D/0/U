FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    impellerWall
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }

    baffleWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    insideSlider
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }

    outsideSlider
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }

    defaultFaces
    {
        type            empty;
    }
}

// ************************************************************************* //
