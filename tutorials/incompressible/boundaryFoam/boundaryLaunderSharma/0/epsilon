FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 10;

boundaryField
{
    lowerWall
    {
        type            fixedValue;
        value           uniform 1e-08;
    }

    upperWall
    {
        type            fixedValue;
        value           uniform 1e-08;
    }

    frontBack
    {
        type            cyclic;
    }

    defaultFaces
    {
        type            empty;
    }
}

