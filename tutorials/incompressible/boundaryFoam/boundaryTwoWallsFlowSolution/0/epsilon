FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 14.855;

boundaryField
{
    lowerWall
    {
        type            epsilonWallFunction;
        refValue        uniform 0;
        value           uniform 14.855;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
    }
    upperWall
    {
        type            epsilonWallFunction;
        refValue        uniform 2.37003e-310;
        value           uniform 14.855;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
    }
    flow
    {
        type            cyclic;
        value           uniform 14.855;
    }
    defaultFaces
    {
        type            empty;
    }
}

