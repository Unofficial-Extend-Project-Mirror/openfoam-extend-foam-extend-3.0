FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    // Solver for the U equation
    U PCG
    {
        preconditioner
        {
            preconditioner DIC;
        }

        minIter          0;
        maxIter          2000;
        tolerance        1e-06;
        relTol           0;
    };
    // Solver for the k equation
    k PBiCG
    {
        preconditioner
        {
            preconditioner DILU;
        }

        minIter          0;
        maxIter          500;
        tolerance        1e-06;
        relTol           0;
    };
    // Solver for the epsilon equation
    epsilon PBiCG
    {
        preconditioner
        {
            preconditioner DILU;
        }

        minIter          0;
        maxIter          500;
        tolerance        1e-06;
        relTol           0;
    };
    // Solver for the R equation
    R PBiCG
    {
        preconditioner
        {
            preconditioner DILU;
        }

        minIter          0;
        maxIter          500;
        tolerance        1e-06;
        relTol           0;
    };
    // Solver for the nuTilda equation
    nuTilda PBiCG
    {
        preconditioner
        {
            preconditioner DILU;
        }

        minIter          0;
        maxIter          500;
        tolerance        1e-06;
        relTol           0;
    };
}

PISO
{
    // Number of PISO correctors
    nCorrectors     2;
    // Number of non-orthogonal correctors
    nNonOrthogonalCorrectors 0;
    // momentumPredictor?
    momentumPredictor yes;
    // fluxGradp?
    fluxGradp       no;
}

relaxationFactors
{
    // U
    U               0.5;
    // k
    k               0.7;
    // epsilon
    epsilon         0.7;
    // R
    R               0.7;
    // nuTilda
    nuTilda         0.7;
}

