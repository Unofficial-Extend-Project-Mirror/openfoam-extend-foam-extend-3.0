FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 3.5;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 3.5;
    }
    outlet
    {
        type            zeroGradient;
    }
    innerWall
    {
        type            omegaWallFunction;
        U               Urel;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        beta1           0.075;
        value           uniform 3.5;
    }
    outerWall
    {
        type            omegaWallFunction;
        U               Urel;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        beta1           0.075;
        value           uniform 3.5;
    }
    cyclic
    {
        type            cyclic;
        value           uniform 3.5;
    }
}

