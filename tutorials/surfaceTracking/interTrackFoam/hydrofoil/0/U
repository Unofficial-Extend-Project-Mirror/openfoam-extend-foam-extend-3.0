FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 1 -1 0 0 0 0];

internalField       uniform (0.8 0 0);

referenceLevel      (0 0 0);

boundaryField
{
    bottom
    {
        type slip;
    }

    hydrofoil
    {
        type fixedValue;
        value uniform (0 0 0);
    }

    freeSurface
    {
        type fixedGradient;
        gradient uniform (0 0 0);
    }

    outlet
    {
        type zeroGradient;
    }

    inlet
    {
        type fixedValue;
        value uniform (0.8 0 0);
    }

    frontAndBackPlanes
    {
        type empty;
    }
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
