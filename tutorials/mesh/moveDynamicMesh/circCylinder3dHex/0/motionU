FoamFile
{
    version     2.0;
    format      ascii;
    class       tetPointVectorField;
    object      motionU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    bottomWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    sideWall
    {
        type            zeroGradient; //slip;
    }

    topWall
    {
        type            oscillatingFixedValue;
        refValue        uniform (0 0 0);
        amplitude       uniform (0 0 0.5);
        frequency       0.1;
        //type            fixedValue;
        //value           uniform (0 0 0.1);
    }

    outlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}

// ************************************************************************* //
