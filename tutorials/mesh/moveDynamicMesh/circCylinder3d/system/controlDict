FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application    moveDynamicMesh;

startTime           0;

startFrom           latestTime;

endTime             20.0;

stopAt              endTime;

deltaT              0.1;

adjustTimeStep      no;

maxCo               0.8;

writeControl        timeStep;

writeInterval       20;

writeFormat         ascii;

writeCompression    uncompressed;

timeFormat          general;

timePrecision       11;

runTimeModifiable   yes;

