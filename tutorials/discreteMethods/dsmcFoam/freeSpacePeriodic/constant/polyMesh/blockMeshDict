FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 0.01;

vertices
(
    (-5 -4 -4)
    (5 -4 -4)
    (5 4 -4)
    (-5 4 -4)
    (-5 -4 4)
    (5 -4 4)
    (5 4 4)
    (-5 4 4)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (20 16 16) simpleGrading (1 1 1)
);

boundary
(
    xPeriodic
    {
        type cyclic;
        faces
        (
        (1 2 6 5)
        (0 4 7 3)
        );
    }

    yPeriodic
    {
        type cyclic;
        faces
        (
        (2 3 7 6)
        (0 1 5 4)
        );
    }

    zPeriodic
    {
        type cyclic;
        faces
        (
            (4 5 6 7)
            (0 3 2 1)
        );
    }
)

mergePatchPairs
(
);

