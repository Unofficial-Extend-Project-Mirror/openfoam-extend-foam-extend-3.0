FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      dsmcRhoN;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    flow
    {
        type            zeroGradient;
    }

    entrance
    {
        type            symmetryPlane;
    }

    walls
    {
        type            zeroGradient;
    }
}

