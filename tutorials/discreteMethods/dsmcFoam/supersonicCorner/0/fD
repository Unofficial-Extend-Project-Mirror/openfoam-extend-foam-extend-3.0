FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      fD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    flow
    {
        type            calculated;
        value           uniform (0 0 0);
    }

    entrance
    {
        type            symmetryPlane;
    }

    walls
    {
        type            calculated;
        value           uniform (0 0 0);
    }
}

