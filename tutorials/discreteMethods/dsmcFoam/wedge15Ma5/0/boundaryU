FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      boundaryU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    flow
    {
        type            fixedValue;
        value           uniform (1736 0 0);
    }

    obstacle
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    periodic
    {
        type            cyclic;
    }
}

