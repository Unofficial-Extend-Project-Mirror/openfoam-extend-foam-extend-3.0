FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alphaSgs;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }

    outlet
    {
        type            zeroGradient;
    }

    upperWall
    {
        type            alphaSgsJayatillekeWallFunction;
        value           uniform 0;
    }

    lowerWall
    {
        type            alphaSgsJayatillekeWallFunction;
        value           uniform 0;
    }

    frontAndBack
    {
        type            cyclic;
    }
}

