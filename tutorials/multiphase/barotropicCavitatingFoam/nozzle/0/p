FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [1 -1 -2 0 0 0 0];

internalField       uniform 1e5;

boundaryField
{
    in
    {
        type            fixedValue;
        value           uniform 10e5;
    }
    out
    {
        type            fixedValue;
        value           uniform 1e5;
    }
    bottom
    {
        type            symmetryPlane;
    }
    topWall
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
}

