FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [ 0 2 -3 0 0 0 0 ];

internalField       uniform 10.0;

boundaryField
{
    bottom
    {
        type               fixedValue;
        value              uniform 10.0;
    }

    top
    {
        type               inletOutlet;
        phi                phia;
        inletValue         uniform 10.0;
        value              uniform 10.0;
    }

    walls
    {
        type               zeroGradient;
    }

    frontBack
    {
        type               empty;
    }

}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
