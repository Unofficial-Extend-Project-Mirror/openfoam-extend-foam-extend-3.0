FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -3 0 0 0 0 ];

internalField   uniform 20;

boundaryField
{
    rotor
    {
        type            compressible::epsilonWallFunction;
        value           uniform 20;
    }

    stator
    {
        type            compressible::epsilonWallFunction;
        value           uniform 20;
    }

    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }
}

