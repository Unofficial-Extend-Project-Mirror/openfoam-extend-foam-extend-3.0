FoamFile
{
    version     2.0;
    format      ascii;
    class       volTensorField;
    location    "0";
    object      materialDirections;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform ( 1 0 0
			  0 1 0
			  0 0 1 );

boundaryField
{
    front
    {
      type            empty;
      //type            zeroGradient;
    }
    back
    {
      type            empty;
      //type            zeroGradient;
    }
    left
    {
        type            zeroGradient;
    }
    bottom
    {
        type            zeroGradient;
    }
    right
    {
        type            zeroGradient;
    }
    top
    {
        type            zeroGradient;
    }
}

