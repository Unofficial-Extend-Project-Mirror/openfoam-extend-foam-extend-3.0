FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      rheologyProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

planeStress     no;

rheology
{
    type                    multiMaterial;

    laws
    (

     substrate
     {
       type            linearElastic;
       rho             rho [1 -3 0 0 0 0 0] 1500;
       E               E [1 -1 -2 0 0 0 0] 200e+9;
       nu              nu [0 0 0 0 0 0 0] 0.3; //0.38
     }

     adhesive
     {
       type            linearElastic;
       rho             rho [1 -3 0 0 0 0 0] 1500;
       E               E [1 -1 -2 0 0 0 0] 1e+9;
       nu              nu [0 0 0 0 0 0 0] 0.3;
     }
     );
}

