FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPoint;

surfaceFormat     dx;

surfaces
(
    constantPlane
    {
        name      plate;
        basePoint (0 0 0.25);
        normalVector (0 0 1);
    }
);

fields
(
    sigmaxx
);

