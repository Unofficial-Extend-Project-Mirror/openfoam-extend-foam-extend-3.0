FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    left
      {
	type     fixedDisplacement;
	value    uniform (0 0 0);
      }

    right
      {
	type      solidTraction;
	nonLinear totalLagrangian;
	traction  uniform (0 -1e8 0);
	pressure  uniform 0;
	value     uniform (0 0 0);
      }

    top
      {
	type      solidTraction;
	nonLinear totalLagrangian;
	traction  uniform (0 0 0);
	pressure  uniform 0;
	value     uniform (0 0 0);
      }

    bottom
      {
	type      solidTraction;
	nonLinear totalLagrangian;
	traction  uniform (0 0 0);
	pressure  uniform 0;
	value     uniform (0 0 0);
      }

    frontAndBack
      {
	type empty;
      }
}

// ************************************************************************* //
