FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    blockT
    {
        solver           BiCGStab;
        preconditioner   Cholesky;

        tolerance        1e-09;
        relTol           0.01;

        minIter          1;
        maxIter          1000;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 0;
}

relaxationFactors
{
    T    0.9;
    Ts   0.9;
}

